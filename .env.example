# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=owner/repo
DEFAULT_BRANCH=main

# MCP Configuration
MEMORY_MCP_URL=stdio://memory-mcp
GITHUB_MCP_URL=stdio://github-mcp
PLAYWRIGHT_MCP_URL=stdio://playwright-mcp

# Development
NODE_ENV=development
LOG_LEVEL=info

# Agent Models (Optional)
ARCHITECT_MODEL=gpt-4
DEVELOPER_MODEL=claude-3-opus
TESTER_MODEL=gpt-4
DEBUGGER_MODEL=claude-3-opus

# API Keys (Optional)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Pipeline Configuration
MAX_RETRIES=3
CHECKPOINT_INTERVAL=5
TIMEOUT_MINUTES=30

# Feature Flags
ENABLE_PARALLEL_AGENTS=true
ENABLE_AUTO_RECOVERY=true
ENABLE_METRICS=true